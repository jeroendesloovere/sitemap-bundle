{
  "name": "jeroendesloovere/sitemap-bundle",
  "type": "library",
  "description": "Symfony bundle for generating (one or multiple) Sitemaps and a SitemapIndex.",
  "keywords": ["sitemap", "generator", "symfony", "xml"],
  "homepage": "https://github.com/jeroendesloovere/sitemap-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Jeroen Desloovere",
      "email": "info@jeroendesloovere.be",
      "homepage": "https://jeroendesloovere.be",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "symfony/framework-bundle": "3.*",
    "symfony/console": "3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "squizlabs/php_codesniffer": "^3.2",
    "mikey179/vfsstream": "^1.6"
  },
  "autoload": {
    "psr-4": { "JeroenDesloovere\\SitemapBundle\\": "src/" }
  }
}
